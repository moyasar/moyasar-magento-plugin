                </label>
                <comment>
                    <![CDATA[<strong> Magento 2 payment module that integrate with <a target="_blank" href="https:\\moyasar.com">Moyasar</a> Gateway.</strong>]]></comment>
                <group id="moyasar_payments_configuration" showInDefault="1" showInStore="1" showInWebsite="1"
                       sortOrder="2" translate="label">
                    <label>Configuration</label>

                    <field id="publishable_api_key" translate="label" type="text" sortOrder="4" showInDefault="1"
                           showInWebsite="1" showInStore="0">
                        <label>Publishable API Key</label>
                        <config_path>payment/moyasar_payments/publishable_api_key</config_path>
                    </field>

                    <field id="secret_api_key" translate="label" type="text" sortOrder="3" showInDefault="1"
                           showInWebsite="1" showInStore="0">
                        <label>Secret API Key</label>
                        <config_path>payment/moyasar_payments/secret_api_key</config_path>
                    </field>

                    <field id="generate_invoice" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="6"
                           translate="label" type="select">
                        <label>Invoice Generation</label>
                        <comment>Generating invoice of completed order automatically.</comment>
                        <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                        <config_path>payment/moyasar_payments/generate_invoice</config_path>
                    </field>

                    <field id="auto_void" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="7"
                           translate="label" type="select">
                        <label>Auto Void</label>
                        <config_path>payment/moyasar_payments/auto_void</config_path>
                        <comment>Enable Auto void for payments that do not pass currency/amount check.</comment>
                        <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                    </field>

                    <field id="webhook_url" translate="label" type="text" sortOrder="8" showInDefault="1"
                           showInWebsite="1" showInStore="0">
                        <label>Webhook URL</label>
                        <config_path>payment/moyasar_payments/webhook_url</config_path>
                        <frontend_model>Moyasar\Magento2\Admin\DisabledElement</frontend_model>
                        <comment>Webhook URL for receiving payment status updates.</comment>
                    </field>

                    <field id="webhook_secret" translate="label" type="text" sortOrder="9" showInDefault="1"
                           showInWebsite="1" showInStore="0">
                        <label>Webhook Secret</label>
                        <config_path>payment/moyasar_payments/webhook_secret</config_path>
                        <frontend_model>Moyasar\Magento2\Admin\DisabledElement</frontend_model>
                    </field>

                    <field id="enable_cron" showInDefault="1" showInStore="0" showInWebsite="1" sortOrder="10"
                           translate="label" type="select">
                        <label>Enable Cron Job</label>
                        <config_path>payment/moyasar_payments/enable_cron</config_path>
                        <comment>Enable Cron job for checking pending payments</comment>
                        <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                    </field>


                </group>
                <group id="moyasar_payments" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="3"
                       translate="label">
                    <label>Credit Card</label>
                    <field id="active" showInDefault="1" showInStore="1" showInWebsite="0" sortOrder="1"
                           translate="label" type="select">
                        <label>Enabled</label>
                        <config_path>payment/moyasar_payments/active</config_path>
                        <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                    </field>

                    <field id="title" translate="label" type="text" sortOrder="2" showInDefault="1" showInWebsite="1"
                           showInStore="1">
                        <label>Card Payment Title</label>
                        <config_path>payment/moyasar_payments/title</config_path>
                        <comment>Card payment title in checkout page.</comment>
                    </field>

                    <field id="schemes" translate="label" type="multiselect" sortOrder="3" showInDefault="1"
                           showInWebsite="1" showInStore="0">
                        <label>Schemes</label>
                        <config_path>payment/moyasar_payments/schemes</config_path>
                        <source_model>Moyasar\Magento2\Model\Config\Schemes</source_model>
                        <can_be_empty>0</can_be_empty>
                    </field>

                </group>
                <group id="moyasar_payments_apple_pay" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="4"
                       translate="label">
                    <label>Apple Pay</label>
                    <field id="active" showInDefault="1" showInStore="1" showInWebsite="0" sortOrder="2"
                           translate="label" type="select">
                        <label>Enabled</label>
                        <config_path>payment/moyasar_payments_apple_pay/active</config_path>
                        <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                    </field>

                    <field id="title" translate="label" type="text" sortOrder="2" showInDefault="1" showInWebsite="1"
                           showInStore="1">
                        <label>Apple Pay Title</label>
                        <config_path>payment/moyasar_payments_apple_pay/title</config_path>
                        <comment>Apple Pay payment title in checkout page.</comment>
                    </field>
                </group>
                <group id="moyasar_payments_stc_pay" showInDefault="1" showInStore="1" showInWebsite="1" sortOrder="5"
                       translate="label">

                    <label>STC Pay</label>
                    <field id="active" showInDefault="1" showInStore="1" showInWebsite="0" sortOrder="2"
                           translate="label" type="select">
                        <label>Enabled</label>
                        <config_path>payment/moyasar_payments_stc_pay/active</config_path>
                        <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                    </field>

                    <field id="title" translate="label" type="text" sortOrder="2" showInDefault="1" showInWebsite="1"
                           showInStore="1">
                        <label>STC Pay Title</label>
                        <config_path>payment/moyasar_payments_stc_pay/title</config_path>
                        <comment>STC Pay payment title in checkout page.</comment>
                    </field>
                </group>
            </group>

        </section>
    </system>
</config>
